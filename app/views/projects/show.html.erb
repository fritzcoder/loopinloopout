<div class="page">	
<div>
<%= render 'project_header' %>
<%= render 'project_nav' %>
</div>
<div>
	<%= render 'layouts/small_player' %>
</div>
<br />
<% if @project.type == "Collaboration"%>
<%= render 'collaboration' %>
<% elsif @project.type == "Remix" %>
<%= render 'remix' %>
<% elsif @project.type == "RemixChallenge" %>
<%= render 'remix_challenge' %>
<% elsif @project.type == "Challenge" %>
<%= render 'challenge' %>
<% else %>
<%= render 'project' %>
<% end %>
</div>